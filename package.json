{
  "name": "poker-face",
  "version": "1.0.0",
  "description": "Command line tool to calculate the name of a hand of poker",
  "scripts": {
    "build": "tsc",
    "lint": "eslint --fix",
    "test": "jest"
  },
  "bin": {
    "poker": "./dist/entrypoint.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/smcveigh941/poker-face.git"
  },
  "author": "smcveigh941",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/smcveigh941/poker-face/issues"
  },
  "homepage": "https://github.com/smcveigh941/poker-face#readme",
  "jest": {
    "preset": "ts-jest",
    "collectCoverageFrom": [
      "server/**/*.{ts,js,jsx,mjs}"
    ],
    "testMatch": [
      "<rootDir>/**/?(*.)(spec|test).{ts,js,jsx,mjs}"
    ],
    "testEnvironment": "node",
    "moduleFileExtensions": [
      "web.js",
      "js",
      "json",
      "node",
      "ts"
    ]
  },
  "dependencies": {
    "@types/jest": "^27.4.1",
    "@types/node": "^17.0.21",
    "@types/yargs": "^17.0.9",
